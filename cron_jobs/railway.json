{
	"$schema": "https://schema.up.railway.app/railway.schema.json",
	"build": {
		"builder": "DOCKERFILE",
		"dockerfilePath": "cron_jobs/Dockerfile"
	},
	"deploy": {
		"startCommand": "PYTHONPATH=. python3 app/scraping.py",
		"numReplicas": 1,
		"cronSchedule": "0 * * * *",
		"sleepApplication": true
	}
}