{
  "services": [
    {
      "name": "api",
      "path": "./api",
      "start": "uvicorn app.main:app --host 0.0.0.0 --port 8000"
    },
    {
      "name": "scraping",
      "path": "./cron-jobs",
      "start": "PYTHONPATH=. python3 app/scripts/scraping.py",
      "schedule": "0 * * * *"
    }
  ]
}